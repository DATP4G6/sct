function Setup() -> void {
    int i = 0;
    while (i < ##COUNT##) {
        create TestAgent::A();
        i = i + 1;
    }
    create Counter::Counting(time: 0);
}

class TestAgent() {
    state A {
        enter A;
    }
    state B {
        enter B;
    }
}

class Counter(int time) {
    state Counting {
        time = time + 1;
        if ((time > 10000) + (!exists(TestAgent::?()))) {
            exit;
        }
        enter Counting;
    }
}
